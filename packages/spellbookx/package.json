{
  "name": "spellbookx",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "spellbookx": "./dist/index.js"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "inquirer": "^12.11.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9"
  },
  "nx": {
    "tags": [
      "cli"
    ],
    "targets": {
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "{projectRoot}",
          "command": "node dist/index.js"
        }
      }
    }
  }
}
